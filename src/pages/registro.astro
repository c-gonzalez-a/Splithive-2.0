---
import Registro from "../components/registro"
import Layout from "../layouts/Layout.astro"
import  "../styles/global.css"

const grupo = Astro.url.searchParams.get('grupo');
const invitado = Astro.url.searchParams.get('invitado');
---
<Layout title="Registro">
    <main>
        <Registro client:only grupo={grupo} invitado={invitado}></Registro>
    </main>
</Layout>